{
    "name": "RhubarbPHP/website",
    "description": "The website for Rhubarb PHP",
    "minimum-stability": "dev",
    "license": "Apache 2.0",
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/RhubarbPHP/Module.JsValidation.git"
        }
    ],
    "require-dev": {
        "codeception/codeception": "*"
    },
    "require":
    {
        "rhubarbphp/rhubarb": "^1.0.0",
        "rhubarbphp/module-leaf": "*",
        "erusev/parsedown-extra" : "*",
        "monolog/monolog": "^1",
        "rhubarbphp/module-stem": "*",
        "rhubarbphp/module-leaf-common-controls": "1.0.0.x-dev",
        "rhubarbphp/custard": "^1.0",
        "rhubarbphp/scaffold-application-settings": "*"
    },
    "autoload":
    {
        "psr-4":
        {
            "Rhubarb\\Website\\": "src/"
        }
    },
    "config": {
        "bin-dir": "bin/"
    }
}
